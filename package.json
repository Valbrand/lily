{
  "publisher": "valbrand",
  "name": "lily",
  "displayName": "lily",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.42.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:clojure"
  ],
  "main": "./out/boundaries/vscode/extension.js",
  "contributes": {
    "configuration": {
      "title": "Lily",
      "properties": {
        "lily.featureFlags.bleedingEdge": {
          "type": "boolean",
          "description": "Enable unfinished features",
          "default": false,
          "scope": "resource"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "preintegration": "npm run compile && npm run lint",
    "integration": "node ./out/test/runTest.js",
    "test": "jest --coverage"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/jest": "^25.1.3",
    "@types/mocha": "^7.0.1",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.42.0",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "jest": "^25.1.0",
    "mocha": "^10.2.0",
    "ts-jest": "^25.2.1",
    "typescript": "^3.7.5",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "@cycle/run": "^5.4.0",
    "parinfer": "^3.12.0",
    "xstream": "^11.11.0"
  }
}
